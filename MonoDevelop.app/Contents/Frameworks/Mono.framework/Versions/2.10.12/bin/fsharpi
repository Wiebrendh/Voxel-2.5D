#!/bin/sh
EXEC="exec "

if test x$1 = x--debug; then
   DEBUG=--debug
   shift
fi

if test x$1 = x--gdb; then
   shift
   EXEC="gdb --eval-command=run --args "
fi

if test x$1 = x--valgrind; then
  shift
  EXEC="valgrind $VALGRIND_OPTIONS"   
fi


$EXEC mono $DEBUG $MONO_OPTIONS "/Users/user/Desktop/voxel-2.5d/MonoDevelop.app/Contents/MacOS/../Frameworks/Mono.framework"/Versions/2.10.12/lib/mono/4.0/fsi.exe $@
